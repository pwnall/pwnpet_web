<%= form_for(@command_result) do |f| %>
  <% if @command_result.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@command_result.errors.count, "error") %> prohibited this command_result from being saved:</h2>

      <ul>
      <% @command_result.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :shell_session_id %><br />
    <%= f.number_field :shell_session_id %>
  </div>
  <div class="field">
    <%= f.label :command %><br />
    <%= f.text_area :command %>
  </div>
  <div class="field">
    <%= f.label :stdin %><br />
    <%= f.text_area :stdin %>
  </div>
  <div class="field">
    <%= f.label :stdout %><br />
    <%= f.text_area :stdout %>
  </div>
  <div class="field">
    <%= f.label :stderr %><br />
    <%= f.text_area :stderr %>
  </div>
  <div class="field">
    <%= f.label :exit_code %><br />
    <%= f.number_field :exit_code %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
